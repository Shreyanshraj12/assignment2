    "def maximumProduct(nums):\n",
    "    nums.sort()  \n",
    "    n = len(nums)\n",
    "    return max(nums[n-1] * nums[n-2] * nums[n-3], nums[0] * nums[1] * nums[n-1])\n",
    "\n",
    "\n",
    "nums = [1, 2, 3]\n",
    "print(maximumProduct(nums)) "
   ]
  }
